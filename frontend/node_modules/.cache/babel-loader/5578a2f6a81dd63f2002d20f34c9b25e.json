{"ast":null,"code":"import { signIn, signOut } from \"@/utils\";\nexport default {\n  name: \"HeaderComp\",\n  props: [\"accountId\", \"isSignedIn\"],\n  filters: {\n    shortAddress: function (value) {\n      if (!value) return '';\n\n      if (value.length > 25) {\n        return value.substring(0, 5) + '...' + value.substring(value.length - 5);\n      }\n\n      return value;\n    }\n  },\n  methods: {\n    async login() {\n      if (!this.isSignedIn) {\n        await signIn();\n      }\n    },\n\n    async signOut() {\n      await signOut();\n      this.$parent.updateInfo();\n    }\n\n  },\n  computed: {\n    appName() {\n      return process.env.VUE_APP_APP_NAME;\n    },\n\n    shortAddressId() {\n      return this.$options.filters.shortAddress(this.accountId);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoEA,SAAQA,MAAR,EAAgBC,OAAhB,QAA8B,SAA9B;AAEA,eAAe;EACbC,IAAI,EAAE,YADO;EAEbC,KAAK,EAAC,CAAC,WAAD,EAAc,YAAd,CAFO;EAGbC,OAAO,EAAE;IACPC,YAAY,EAAE,UAAUC,KAAV,EAAiB;MAC7B,IAAI,CAACA,KAAL,EAAY,OAAO,EAAP;;MACZ,IAAGA,KAAK,CAACC,MAAN,GAAe,EAAlB,EAAqB;QACnB,OAAOD,KAAK,CAACE,SAAN,CAAgB,CAAhB,EAAmB,CAAnB,IAAwB,KAAxB,GAAgCF,KAAK,CAACE,SAAN,CAAgBF,KAAK,CAACC,MAAN,GAAe,CAA/B,CAAvC;MACF;;MACA,OAAOD,KAAP;IACF;EAPO,CAHI;EAYbG,OAAO,EAAC;IACN,MAAMC,KAAN,GAAa;MACX,IAAG,CAAC,KAAKC,UAAT,EAAoB;QAClB,MAAMX,MAAM,EAAZ;MACF;IACD,CALK;;IAMN,MAAMC,OAAN,GAAe;MACb,MAAMA,OAAO,EAAb;MACA,KAAKW,OAAL,CAAaC,UAAb;IACD;;EATK,CAZK;EAuBbC,QAAQ,EAAC;IACPC,OAAO,GAAE;MACP,OAAOC,OAAO,CAACC,GAAR,CAAYC,gBAAnB;IACD,CAHM;;IAIPC,cAAc,GAAE;MACd,OAAO,KAAKC,QAAL,CAAchB,OAAd,CAAsBC,YAAtB,CAAmC,KAAKgB,SAAxC,CAAP;IACF;;EANO;AAvBI,CAAf","names":["signIn","signOut","name","props","filters","shortAddress","value","length","substring","methods","login","isSignedIn","$parent","updateInfo","computed","appName","process","env","VUE_APP_APP_NAME","shortAddressId","$options","accountId"],"sourceRoot":"","sources":["/var/www/ch-8-car-renting/frontend/src/components/HeaderComp.vue"],"sourcesContent":["<template>\n  <header id=\"header\" class=\"d-flex align-items-center\">\n    <div class=\"container d-flex align-items-center\">\n\n      <h1 class=\"logo me-auto\">\n        <router-link :to=\"{name:'home'}\">{{appName}}</router-link>\n      </h1>\n      <!-- Uncomment below if you prefer to use an image logo -->\n      <!-- <a href=\"index.html\" class=\"logo me-auto\"><img src=\"assets/img/logo.png\" alt=\"\" class=\"img-fluid\"></a>-->\n\n      <nav id=\"navbar\" class=\"navbar\">\n        <ul>\n          <li>\n            <router-link\n                :to=\"{name:'home'}\"\n                custom\n                v-slot=\"{ href, navigate, isExactActive }\">\n              <a :href=\"href\" @click=\"navigate\"\n                 class=\"nav-link scrollto\" :class=\"isExactActive ? 'active' : ''\" >Home</a>\n            </router-link>\n          </li>\n          <li>\n            <router-link\n                :to=\"{name:'about'}\"\n                custom\n                v-slot=\"{ href, navigate, isExactActive }\">\n              <a :href=\"href\" @click=\"navigate\"\n                 class=\"nav-link scrollto\" :class=\"isExactActive ? 'active' : ''\" >About</a>\n            </router-link>\n          </li>\n          <li>\n            <router-link\n                :to=\"{name:'meetings'}\"\n                custom\n                v-slot=\"{ href, navigate, isExactActive }\">\n              <a :href=\"href\" @click=\"navigate\"\n                 class=\"nav-link scrollto\" :class=\"isExactActive ? 'active' : ''\" >Meetings</a>\n            </router-link>\n          </li>\n          <li class=\"dropdown\" style=\"margin-left: 30px\"  v-if=\"isSignedIn\">\n            <a href=\"#\" class=\"text-decoration-underline\"><span class=\"mx-2\">{{ shortAddressId }}</span> <font-awesome-icon icon=\"caret-down\" /></a>\n            <ul class=\"p-2\">\n              <li><button class=\"btn btn-outline-success w-100\" v-clipboard:copy=\"accountId\" >{{ shortAddressId }} <font-awesome-icon icon=\"copy\" /></button></li>\n              <hr>\n              <li>\n                <router-link\n                    :to=\"{name:'profile-dashboard'}\"\n                    custom\n                    v-slot=\"{ href, navigate, isExactActive }\">\n                  <a :href=\"href\" @click=\"navigate\"\n                     class=\"nav-link scrollto\" :class=\"isExactActive ? 'active' : ''\" >Profile</a>\n                </router-link>\n              </li>\n<!--              <li><a href=\"#\">Drop Down 3</a></li>-->\n              <hr>\n              <li v-if=\"isSignedIn\"><button class=\"btn btn-outline-danger w-100\"  @click.prevent=\"signOut\" >Sign Out</button></li>\n            </ul>\n          </li>\n          <li style=\"margin-left: 30px\" v-else><button class=\"btn btn-outline-success m-2\" @click.prevent=\"login\" >Log In</button></li>\n        </ul>\n        <i class=\"bi bi-list mobile-nav-toggle\"></i>\n      </nav><!-- .navbar -->\n\n    </div>\n  </header>\n</template>\n\n<script>\nimport {signIn, signOut} from \"@/utils\";\n\nexport default {\n  name: \"HeaderComp\",\n  props:[\"accountId\", \"isSignedIn\"],\n  filters: {\n    shortAddress: function (value) {\n      if (!value) return ''\n      if(value.length > 25){\n        return value.substring(0, 5) + '...' + value.substring(value.length - 5);\n      }\n      return value;\n    }\n  },\n  methods:{\n    async login(){\n      if(!this.isSignedIn){\n        await signIn();\n      }\n    },\n    async signOut(){\n      await signOut();\n      this.$parent.updateInfo();\n    },\n  },\n  computed:{\n    appName(){\n      return process.env.VUE_APP_APP_NAME\n    },\n    shortAddressId(){\n      return this.$options.filters.shortAddress(this.accountId)\n    }\n  }\n}\n</script>\n\n<style scoped>\n\n\n/*--------------------------------------------------------------\n# Header\n--------------------------------------------------------------*/\n#header {\n  background: #fff;\n  transition: all 0.5s;\n  z-index: 997;\n  height: 70px;\n}\n#header.header-scrolled {\n  box-shadow: 0px 2px 15px rgba(0, 0, 0, 0.1);\n}\n#header .logo {\n  font-size: 30px;\n  margin: 0;\n  padding: 0;\n  line-height: 1;\n  font-weight: 700;\n  letter-spacing: 1px;\n  text-transform: uppercase;\n}\n#header .logo a {\n  color: #5cb874;\n}\n#header .logo img {\n  max-height: 40px;\n}\n\n.scrolled-offset {\n  margin-top: 70px;\n}\n\n\n/*--------------------------------------------------------------\n# Navigation Menu\n--------------------------------------------------------------*/\n/**\n* Desktop Navigation\n*/\n.navbar {\n  padding: 0;\n}\n.navbar ul {\n  margin: 0;\n  padding: 0;\n  display: flex;\n  list-style: none;\n  align-items: center;\n}\n.navbar li {\n  position: relative;\n}\n.navbar a, .navbar a:focus {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  padding: 10px 0 10px 30px;\n  font-size: 15px;\n  color: #222222;\n  white-space: nowrap;\n  transition: 0.3s;\n}\n.navbar a i, .navbar a:focus i {\n  font-size: 12px;\n  line-height: 0;\n  margin-left: 5px;\n}\n.navbar a:hover, .navbar .active, .navbar .active:focus, .navbar li:hover > a {\n  color: #5cb874;\n}\n.navbar .getstarted, .navbar .getstarted:focus {\n  color: #5cb874;\n  padding: 8px 25px;\n  margin-left: 30px;\n  border-radius: 4px;\n  border: 2px solid #5cb874;\n  transition: 0.3s;\n  font-size: 14px;\n}\n.navbar .getstarted:hover, .navbar .getstarted:focus:hover {\n  background: #5cb874;\n  color: #fff;\n}\n.navbar .dropdown ul {\n  display: block;\n  position: absolute;\n  left: 14px;\n  top: calc(100% + 30px);\n  margin: 0;\n  padding: 10px 0;\n  z-index: 99;\n  opacity: 0;\n  visibility: hidden;\n  background: #fff;\n  box-shadow: 0px 0px 30px rgba(127, 137, 161, 0.25);\n  transition: 0.3s;\n}\n.navbar .dropdown ul li {\n  min-width: 200px;\n}\n.navbar .dropdown ul a {\n  padding: 10px 20px;\n  text-transform: none;\n}\n.navbar .dropdown ul a i {\n  font-size: 12px;\n}\n.navbar .dropdown ul a:hover, .navbar .dropdown ul .active:hover, .navbar .dropdown ul li:hover > a {\n  color: #676767;\n}\n.navbar .dropdown:hover > ul {\n  opacity: 1;\n  top: 100%;\n  visibility: visible;\n}\n.navbar .dropdown .dropdown ul {\n  top: 0;\n  left: calc(100% - 30px);\n  visibility: hidden;\n}\n.navbar .dropdown .dropdown:hover > ul {\n  opacity: 1;\n  top: 0;\n  left: 100%;\n  visibility: visible;\n}\n@media (max-width: 1366px) {\n  .navbar .dropdown .dropdown ul {\n    left: -90%;\n  }\n  .navbar .dropdown .dropdown:hover > ul {\n    left: -100%;\n  }\n}\n</style>"]},"metadata":{},"sourceType":"module"}